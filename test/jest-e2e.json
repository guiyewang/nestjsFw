{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "DB/mysql-db/(.*)": "<rootDir>/../libs/mysql-db/src/$1",
    "DB/mysql-db": "<rootDir>/../libs/mysql-db/src"
  }
}