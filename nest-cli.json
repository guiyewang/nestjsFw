{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "mysql-db": {
      "type": "library",
      "root": "libs/mysql-db",
      "entryFile": "index",
      "sourceRoot": "libs/mysql-db/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mysql-db/tsconfig.lib.json"
      }
    },
    "websocket": {
      "type": "library",
      "root": "libs/websocket",
      "entryFile": "index",
      "sourceRoot": "libs/websocket/src",
      "compilerOptions": {
        "tsConfigPath": "libs/websocket/tsconfig.lib.json"
      }
    }
  }
}